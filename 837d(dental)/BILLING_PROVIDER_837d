SELECT distinct TransactionID,BillingID,NM1_BillingProviderName.ResponseContactLastorOrganizationName_03 AS BUSINESS_NAME,
NM1_BillingProviderName.ResponseContactIdentifier_09 AS NPI,
REF_BillingProviderTaxIdentification.MemberGrouporPolicyNumber_02 AS TAXID, 
PER_BillingProviderContactInformation.ResponseContactName_02 AS CONTACT_NAME,
PER_BillingProviderContactInformation.ResponseContactCommunicationNumber_04 AS CONTACT_NUMBER,
CONCAT(N3_BillingProviderAddress.ResponseContactAddressLine_01, ',',
N4_BillingProviderCity_State_ZIPCode.AdditionalPatientInformationContactCityName_01,' ',
N4_BillingProviderCity_State_ZIPCode.AdditionalPatientInformationContactStateCode_02,' ',
N4_BillingProviderCity_State_ZIPCode.AdditionalPatientInformationContactPostalZoneorZIPCode_03) AS ADDRESS
FROM billing
WHERE TransactionID = {{transID_d}};