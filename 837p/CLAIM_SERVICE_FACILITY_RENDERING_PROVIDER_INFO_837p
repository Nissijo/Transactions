SELECT ClaimID,ClaimHeaderID,CLM_ClaimInformation.PatientControlNumber_01 AS PATIENT_CONTROL_NUMBER,
(CLM_ClaimInformation.TotalClaimChargeAmount_02) AS TOTAL_BILLED_AMT,
NM1_ServiceFacilityLocationName.ResponseContactLastorOrganizationName_03 AS SERVICE_FACILITY_BUSINESSNAME,
NM1_ServiceFacilityLocationName.ResponseContactIdentifier_09 AS SERVICE_FACILITY_NPI,
CONCAT(N3_ServiceFacilityLocationAddress.ResponseContactAddressLine_01 ,' ',
N4_ServiceFacilityLocationCity_State_ZipCode.AdditionalPatientInformationContactCityName_01,',',
N4_ServiceFacilityLocationCity_State_ZipCode.AdditionalPatientInformationContactStateCode_02,' ',
N4_ServiceFacilityLocationCity_State_ZipCode.AdditionalPatientInformationContactPostalZoneorZIPCode_03) AS SERVICE_FACILITY_ADDRESS,
CONCAT(NM1_RenderingProviderName.ResponseContactLastorOrganizationName_03,',',NM1_RenderingProviderName.ResponseContactFirstName_04,' ',
NM1_RenderingProviderName.ResponseContactMiddleName_05) AS RENDERING_PROVIDER_NAME,
(REF_RenderingProviderSecondaryIdentification.MemberGrouporPolicyNumber_02) AS RENDERING_PROVIDER_NPI
FROM CLAIM
WHERE ClaimID={{CLAIMID}};