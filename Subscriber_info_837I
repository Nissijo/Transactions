SELECT BillingID,ClaimHeaderID,
CONCAT(NM1_SubscriberName.ResponseContactLastorOrganizationName_03,',',NM1_SubscriberName.ResponseContactFirstName_04,' ',NM1_SubscriberName.ResponseContactMiddleName_05) AS NAME,
NM1_SubscriberName.ResponseContactIdentifier_09 AS MEMBER_ID,
(DMG_SubscriberDemographicinformation.DependentGenderCode_03) AS GENDER,
FROM_UNIXTIME(UNIX_TIMESTAMP(DMG_SubscriberDemographicinformation.DependentBirthDate_02, 'yyyyMMdd'), 'MM/dd/yy') AS DOB,
CONCAT(N3_SubscriberAddress.ResponseContactAddressLine_01,',',N4_SubscriberCity_State_ZIPCode.AdditionalPatientInformationContactCityName_01,',',
N4_SubscriberCity_State_ZIPCode.AdditionalPatientInformationContactStateCode_02,',',
N4_SubscriberCity_State_ZIPCode.AdditionalPatientInformationContactPostalZoneorZIPCode_03) AS ADDRESS,
CONCAT(NM1_PayerName.EntityIdentifierCode_01,',',NM1_PayerName.ResponseContactLastorOrganizationName_03) PAYER_SEQUENCE
FROM subscriber_claim_header; 